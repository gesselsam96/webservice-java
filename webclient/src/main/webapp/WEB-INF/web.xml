<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
 PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
 
  <servlet>
  <display-name>ConsumeGreeting</display-name>
    <servlet-name>ConsumeGreeting</servlet-name>
    <servlet-class>com.helloweb.wsclient.ConsumeGreeting</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ConsumeGreeting</servlet-name>
    <url-pattern>/ConsumeGreeting</url-pattern>
  </servlet-mapping>
</web-app>